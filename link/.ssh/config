# for Atea machines
Host 10.66.*.* *.atea.dev
  StrictHostKeyChecking no
  UserKnownHostsFile=/dev/null
  LogLevel ERROR

# Allow MacOS to use the keychain for SSH key passphrases
Host *
  IgnoreUnknown AddKeysToAgent,UseKeychain
  AddKeysToAgent yes
  UseKeychain yes
  AddKeysToAgent yes
  IdentityFile ~/.ssh/id_rsa

