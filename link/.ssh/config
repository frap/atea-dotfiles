# for Atea machines
Host 10.66.*.* tsp* dev* 9.1.1.*
  StrictHostKeyChecking no
  UserKnownHostsFile=/dev/null
  LogLevel ERROR

# Allow MacOS to use the keychain for SSH key passphrases
Host *
  IgnoreUnknown AddKeysToAgent,UseKeychain
  AddKeysToAgent yes
  UseKeychain yes
  LogLevel INFO
  IdentityFile ~/.ssh/id_rsa

